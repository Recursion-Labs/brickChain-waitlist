generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Responses {
  id String @id @default(auto()) @db.ObjectId @map("_id")
  emails String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}